{
  "$schema": "https://json.schemastore.org/prettierrc",
  "tabWidth": 2,
  "printWidth": 100,
  "arrowParens": "always",
  "bracketSameLine": false,
  "bracketSpacing": true,
  "endOfLine": "lf",
  "jsxSingleQuote": false,
  "semi": true,
  "singleAttributePerLine": false,
  "singleQuote": false,
  "trailingComma": "es5",
  "useTabs": false,
  "plugins": [
      "prettier-plugin-tailwindcss"
  ]
}
